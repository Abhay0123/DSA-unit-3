#define checkUpper function
def checkUpper(c):
    if c.isupper():
        return True
    else:
        return False

def solve(num,given_string):
       #write code
    x=0
       #for loop take i as a variable
    for i in range(len(given_string)):
        getString = checkUpper(given_string[i])
        if getString:
           x=x+1           
    print((len(given_string)*x)-x+len(given_string))           #complete the code

  
